  <select id="selectByLitemallCollect"  parameterType="org.linlinjava.litemall.db.domain.LitemallCollect" resultType="org.linlinjava.litemall.db.domain.LitemallCollect" >
  	select 
  	lc.id,
  	lc.add_time as "addTime",
	lu.username  as "userName",
	lg.`name` as "goodName"
 	from litemall_collect lc 
  	LEFT JOIN litemall_user lu on lc.user_id = lu.id 
  	LEFT JOIN litemall_goods lg on lc.value_id = lg.id 
 	where lc.deleted = 0  
 	<if test="userId != null">
       and lc.user_id = #{userId,jdbcType=INTEGER}     
    </if> 
 	<if test="userName != null and userName != '' ">
 		and lu.username like concat('%', #{userName,jdbcType=VARCHAR},'%')
 	</if>
 	<if test="goodName != null and goodName != '' ">
 		and lg.`name` like concat('%', #{goodName,jdbcType=VARCHAR},'%')
 	</if>
  </select>
