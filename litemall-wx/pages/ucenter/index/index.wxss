.cell-access {
  padding: 20rpx 35rpx;
  display: flex;
  background-color: #fff;
}

.cell-access .cell-bd {
  flex: 1;
  line-height: 60rpx;
}

.cell-access .cell-bd-text {
  font-size: 35rpx;
  color: #333;
}

.cell-access .cell-ft-text {
  font-size: 30rpx;
  color: #7f7f7f;
}

.cell-access .cell-ft {
  text-align: right;
  position: relative;
  padding-right: 30rpx;
  right: 25rpx;
  top: 10rpx;
}

.cell-access.cell-sm .cell-bd-text {
  font-size: 30rpx;
}

.cell-access .cell-icon {
  padding-right: 20rpx;
}

.cell-access .cell-img {
  width: 50rpx;
  height: 50rpx;
}

.cell-access .cell-ft::after {
  content: " ";
  display: inline-block;
  height: 6px;
  width: 6px;
  border-width: 1px 1px 0 0;
  border-color: #7f7f7f;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: relative;
  top: -2px;
  position: absolute;
  top: 23rpx;
  margin-top: -4px;
  right: 2px;
}

