/* 标签选择模态框 */
/* 模态框 */
.show-btn {
  margin-top: 100rpx;
  color: #22cc22;
}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 600rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 9999;
  background: #f9f9f9;
  margin: -260rpx 75rpx;
  border-radius: 6rpx;
}

.modal-title {
  font-size: 34rpx;
  color: #030303;
}

.modal-content {
  height: 150rpx;
  flex-direction: row;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0rpx 50rpx 0rpx 50rpx;
}

.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  border-top: 1px solid #dedede;
  font-size: 34rpx;
  line-height: 86rpx;
}

.btn-cancel {
  width: 50%;
  color: #666;
  text-align: center;
  border-right: 1px solid #dedede;
}

.btn-confirm {
  width: 50%;
  color: #ec5300;
  text-align: center;
}

.typicker{
  flex-direction: row;
  display: flex;
  align-items: center;
  padding: 5rpx 10rpx 5rpx 10rpx;
  border-radius: 8rpx;
  background-color: #6bbfe0;
  color: #ffffff;
  font-size: 28rpx;
}