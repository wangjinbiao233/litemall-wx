.evaluation {
  padding: 0 15px;
  background-color: #fff;
}

.evaluation .evaluation-all {
  padding: 30rpx 0;
  display: flex;
  border-bottom: 1px solid #ccc;
}

.evaluation .evaluation-title {
  flex: 1;
  font-size: 35rpx;
  color: #333;
}

.evaluation .evaluation-all-btn {
  text-align: right;
  color: #8ac2d6;
  font-size: 35rpx;
}

.evaluation .evaluation-sort {
  padding: 15rpx 0;
  color: #444;
  font-size: 33rpx;
}

.evaluation .evaluation-sort-msg {
  padding: 25rpx 0 10rpx 0;
}

.evaluation-sort-text {
  font-size: 30rpx;
  color: #929292;
}

.evaluation .evaluation-sort-effect {
  color: #8ac2d6;
  border: 1px solid #dcedf3;
  border-radius: 3px;
  padding: 5rpx 10rpx;
  margin-left: 15rpx;
  font-size: 28rpx;
}

.evaluation-list {
  background-color: #fff;
  white-space: nowrap;
  padding-top: 10px;
}

.evaluation-list .evaluation-box {
  background-color: #f5f5f5;
  width: 390rpx;
  display: inline-block;
  border: 1px solid #f5f5f5;
  border-radius: 5%;
  padding: 16rpx;
  margin-right: 15px;
}

.evaluation-list .evaluation-box:first-child {
  margin-left: 15px;
}

.evaluation-list .evaluation-avatar {
  height: 60rpx;
  width: 60rpx;
  border-radius: 50%;
}

.evaluation-list .evaluation-name {
  font-size: 35rpx;
  color: #333;
  position: relative;
  bottom: 15rpx;
  padding-left: 10rpx;
}

.evaluation-list .evaluation-text {
  padding-top: 10rpx;
  color: #929292;
  height: 105rpx;
  font-size: 30rpx;
  line-height: 1.6;
  white-space: normal;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2; /*是显示多少行数加省略号*/
  -webkit-box-orient: vertical;
}

