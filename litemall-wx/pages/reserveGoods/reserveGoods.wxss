/* reserveGoods.wxss */

.service-header {
  padding: 30rpx 15px;
  border-bottom: 20rpx solid #f8f8f8;
  margin: 0 -15px;
}

.service-title {
  font-size: 38rpx;
  color: #333;
  padding-bottom: 10rpx;
}

.service-describe {
  font-size: 30rpx;
  color: #929292;
}

.goods {
  display: flex;
  justify-content: space-between;
}

.goods-box {
  width: 220rpx;
}

.goods-img {
  margin: 0px;
  width: 100%;
  height: 300rpx;
}

.goods-main {
  flex-direction: column;
  display: flex;
  padding: 10rpx 15rpx;
}

.goods-text {
  display: block;
  line-height: 1.5;
}

.goods-name {
  color: #333;
  font-size: 24rpx;
}

.goods-nameEn {
  color: #929292;
  font-size: 24rpx;
}

.goods-unit {
  font-size: 33rpx;
  color: #8ac2d6;
}

.store-parent {
  background-color: #f4f4f4;
  display: flex;
  flex-flow: row wrap;
  justify-content: flex-start;
  padding: 10rpx;
}

.store-child {
  flex-direction: column;
  display: flex;
  box-sizing: border-box;
  background-color: white;
  flex: 0 0 31.8%;
  margin-left: 10rpx;
  margin-top: 5rpx;
  margin-bottom: 5rpx;
  border-radius: 5rpx;
  box-shadow: 0.5px 0.5px #ddd, 0px 0px #ddd inset;
}
